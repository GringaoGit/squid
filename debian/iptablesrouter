iptables -t nat -I PREROUTING -p tcp --dport 80 -j REDIRECT --to-ports 3128
iptables -t nat -I PREROUTING -p tcp --dport 443 -j REDIRECT --to-ports 3128
iptables -t nat -I PREROUTING --source 192.168.9.0/24 --destination 0/0 -p tcp --dport 80 -j REDIRECT --to-ports 3128
iptables -t nat -I PREROUTING --source 192.168.9.0/24 --destination 0/0 -p tcp --dport 443 -j REDIRECT --to-ports 3128
iptables -t nat -A PREROUTING -i eth1 -p tcp --dport 80 -j DNAT --to 192.168.6.1:3128
iptables -t nat -A PREROUTING -i eth1 -p tcp --dport 443 -j DNAT --to 192.168.6.1:3128
